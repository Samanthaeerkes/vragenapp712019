<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.vragenapp" version="2.1.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Vragen app</name>
    <description>
        Martijn van Schaik
    </description>
    <author email="support@phonegap.com" href="http://phonegap.com">
        PhoneGap Team
    </author>
    <content src="index.html" />
    <splash platform="android" src="splash.png" />
    <splash platform="android" qualifier="port-ldpi" src="res/screen/android/splash-port-ldpi-screen.png" />
    <splash platform="android" qualifier="port-mdpi" src="res/screen/android/splash-port-mdpi-screen.png" />
    <splash platform="android" qualifier="port-hdpi" src="res/screen/android/splash-port-hdpi-screen.png" />
    <splash platform="android" qualifier="port-xhdpi" src="res/screen/android/splash-port-xhdpi-screen.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <script type="javascript">centeredLayout.setGravity(Gravity.BOTTOM);</script>
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="FadeSplashScreen" value="false" />
    <preference name="AutoHideSplashScreen" value="true" />
    <preference name="SplashScreenDelay" value="3000" />
    <preference name="ShowSplashScreen" value="false" />
    <preference name="SplashScreenWidth" value="600" />
    <preference name="SplashScreenHeight" value="300" />
    <preference name="SplashScreenSpinnerColor" value="white" />
</widget>
